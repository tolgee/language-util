{
  "root": true,
  "ignorePatterns": ["node_modules", "*.d.ts", "src/generated"],
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended"],
  "plugins": ["prettier", "jest"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "JSX": true,
    "Proxy": true,
    "RequestInfo": true,
    "RequestInit": true
  },
  "rules": {
    "no-unused-vars": "off",
    "prettier/prettier": "error"
  }
}
